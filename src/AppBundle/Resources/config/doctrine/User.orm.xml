<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\User" table="user">
    <id name="idUser" type="integer" column="id_user">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="20" nullable="false"/>
    <field name="phoneNumber" type="string" column="phone_number" length="12" nullable="false"/>
    <field name="login" type="string" column="login" length="255" nullable="false"/>
    <field name="password" type="string" column="password" length="255" nullable="false"/>
    <field name="roles" type="text" column="roles" nullable="false"/>
  </entity>
</doctrine-mapping>
